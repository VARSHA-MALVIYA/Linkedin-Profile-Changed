{
    "manifest_version": 3,
    "name": "LinkedIn Profile Picture Changer",
    "version": "1.0",
    "description": "Changes all LinkedIn profile pictures to a single image.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.linkedin.com/feed/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    }
  }
  